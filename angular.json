{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kubernetes-dashboard": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "prefix": "kd",
          "styleext": "scss"
        },
        "@schematics/angular:directive": {
          "prefix": "kd"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": ".tmp/frontend",
            "index": "src/app/frontend/index.html",
            "main": "src/app/frontend/index.ts",
            "tsConfig": "src/app/frontend/../../../aio/tsconfig.app.json",
            "polyfills": "src/app/frontend/polyfills.ts",
            "assets": [
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "src/app/frontend/assets",
                "output": "/assets"
              },
              {
                "glob": "robots.txt",
                "input": "src/app/frontend/assets",
                "output": "/"
              }
            ],
            "styles": [
              "node_modules/svg.js/dist/svg.min.js",
              "node_modules/svg.panzoom.js/dist/svg.panzoom.min.js",
              "node_modules/svg.draggable.js/dist/svg.draggable.min.js",
              "src/app/frontend/assets/js/svg.parser.min.js",
              "src/app/frontend/assets/js/svg.import.min.js",
              "src/app/frontend/assets/js/svg.connectable.min.js",
              "src/app/frontend/assets/js/svg.math.min.js",
              "node_modules/highlight.js/styles/github.css",
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/xterm/dist/xterm.css",
              "node_modules/c3/c3.min.css",
              "src/app/frontend/index.scss",
              "src/app/frontend/assets/scss/styles.scss",
              "node_modules/ngx-toastr/toastr.css",
              "src/app/frontend/assets/scss/black-dashboard.scss",
              "src/app/frontend/assets/css/nucleo-icons.css"
            ],
            "scripts": [
              "node_modules/sockjs-client/dist/sockjs.min.js",
              "node_modules/systemjs/dist/s.js",
              "node_modules/systemjs/dist/extras/named-register.js",
              "node_modules/systemjs/dist/extras/amd.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/app/frontend/environments/environment.ts",
                  "with": "src/app/frontend/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kubernetes-dashboard:build",
            "port": 8080
          },
          "configurations": {
            "production": {
              "browserTarget": "kubernetes-dashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kubernetes-dashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/app/frontend/index.spec.ts",
            "karmaConfig": "aio/karma.conf.js",
            "polyfills": "src/app/frontend/polyfills.ts",
            "tsConfig": "src/app/frontend/../../../aio/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/c3/c3.min.css",
              "src/app/frontend/index.scss",
              "src/app/frontend/assets/scss/styles.scss",
              "node_modules/ngx-toastr/toastr.css",
              "src/app/frontend/assets/scss/black-dashboard.scss",
              "src/app/frontend/assets/css/nucleo-icons.css"
            ],
            "assets": [
              "src/favicon.ico",
              "src/app/frontend/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.json"
            ],
            "exclude": []
          }
        }
      }
    },
  },
  "defaultProject": "kubernetes-dashboard",

}
